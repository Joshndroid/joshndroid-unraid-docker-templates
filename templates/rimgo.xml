<?xml version="1.0"?>
<Container version="2">
  <Name>rimgo</Name>
  <Repository>quay.io/pussthecatorg/rimgo</Repository>
  <Registry>https://quay.io/repository/pussthecatorg/rimgo</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/112086-support-joshndroids-docker-repo-support-thread/</Support>  <Project/>
  <Project>https://codeberg.org/video-prize-ranch/rimgo</Project>
  <Overview>Alternative Imgur front-end&#xD;
  &#xD;
  This can further be configured with the environment variables&#xD;
  Please see the project github for further</Overview>
  <Category>Network:Web</Category>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Joshndroid/joshndroid-unraid-docker-templates/main/icons/rimgo.png</Icon>
  <ExtraParams>--restart=always</ExtraParams>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Alternative Imgur front-end&#xD;
  &#xD;
  This can further be configured with the environment variables&#xD;
  Please see the project github for further</Description>
  <Config Name="Port" Target="3000" Default="3000" Mode="tcp" Description="Port" Type="Port" Display="always" Required="false" Mask="false">3000</Config>
  <Config Name="Address" Target="ADDRESS" Default="0.0.0.0" Mode="" Description="Hosted IP Address" Type="Variable" Display="always" Required="false" Mask="false">0.0.0.0</Config>
  <Config Name="Imgur Client ID" Target="IMGUR_CLIENT_ID" Default="546c25a59c58ad7" Mode="" Description="Imgur Client ID" Type="Variable" Display="always" Required="false" Mask="false">546c25a59c58ad7</Config>
  <Config Name="FORCE_WEBP" Target="FORCE_WEBP" Default="0" Mode="" Description="Force WEBP to reduce bandwidth" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
</Container>
