<?xml version="1.0"?>
<Container version="2">
  <Name>Troddit</Name>
  <Repository>bsyed/troddit</Repository>
  <Registry>https://hub.docker.com/r/bsyed/troddit</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>https://forums.unraid.net/topic/112086-support-joshndroids-docker-repo-support-thread/</Support>
  <Project/>https://github.com/burhan-syed/troddit</Project>
  <Overview>A web client for Reddit</Overview>
  <Category>Network:Web</Category>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <TemplateURL></TemplateURL>
  <Icon>https://raw.githubusercontent.com/Joshndroid/joshndroid-unraid-docker-templates/acce11cca57e4000233efb34896e91825a3a5448/icons/troddit.png</Icon>
  <ExtraParams>--restart=always</ExtraParams>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>A web client for Reddit&#xD;
  &#xD;
  This REQUIRES further setup before starting for the first time.&#xD;
  Please see the Environment Variables section in the project github&#xD;
  github.com/burhan-syed/troddit</Description>
  <Config Name="Port" Target="3000" Default="3000" Mode="tcp" Description="Container Port: 3000" Type="Port" Display="always" Required="false" Mask="false">3000</Config>
  <Config Name="CLIENT_ID" Target="CLIENT_ID" Default="" Mode="" Description="Client ID generated on Reddit" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="CLIENT_SECRET" Target="CLIENT_SECRET" Default="" Mode="" Description="Client Secret generated on Reddit" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="REDDIT_REDIRECT" Target="REDDIT_REDIRECT" Default="" Mode="" Description="Reddit Redirection" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="NEXTAUTH_SECRET" Target="NEXTAUTH_SECRET" Default="" Mode="" Description="Nextauth Secret - Generated by running " Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="NEXTAUTH_URL" Target="NEXTAUTH_URL" Default="" Mode="" Description="Container Variable: NEXTAUTH_URL" Type="Variable" Display="always" Required="false" Mask="false"></Config>
</Container>
