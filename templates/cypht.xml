<?xml version="1.0"?>
<Container version="2">
  <Name>Cypht</Name>
  <Repository>sailfrog/cypht-docker:latest</Repository>
  <Registry>https://hub.docker.com/r/sailfrog/cypht-docker/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/112086-support-joshndroids-docker-repo-support-thread/</Support>
  <Project>https://github.com/jasonmunro/cypht-docker</Project>
  <Overview>Lightweight Open Source webmail written in PHP and JavaScript&#xD;
  &#xD;
  All your E-mail, from all your accounts, in one place&#xD;
  Cypht is like a news reader, but for E-mail&#xD;
  Cypht does not replace your existing accounts - it combines them into one&#xD;
  And it's also a news reader&#xD;
  You will need to have a MariaDB database setup prior to using this container</Overview>
  <Category>Network:Web</Category>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Joshndroid/joshndroid-unraid-docker-templates/3dda153d84994beccb8749f9d9fab63a8b8d5940/icons/cypht.png</Icon>
  <ExtraParams>--restart=always</ExtraParams>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Lightweight Open Source webmail written in PHP and JavaScript&#xD;
  &#xD;
  All your E-mail, from all your accounts, in one place&#xD;
  Cypht is like a news reader, but for E-mail&#xD;
  Cypht does not replace your existing accounts it combines them into one&#xD;
  And it's also a news reader&#xD;
  You will need to have a MariaDB database setup prior to using this container</Description>
  <Config Name="Users" Target="/var/lib/hm3/users" Default="/mnt/user/appdata/cypht/users/" Mode="rw" Description="Users Container Path" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/cypht/users/</Config>
  <Config Name="WebUI" Target="80" Default="80" Mode="tcp" Description="WebUI" Type="Port" Display="always" Required="false" Mask="false">80</Config>
  <Config Name="Cypht Username" Target="CYPHT_AUTH_USERNAME" Default="admin" Mode="" Description="Cypht Username" Type="Variable" Display="always" Required="false" Mask="false">admin</Config>
  <Config Name="Cypht Password" Target="CYPHT_AUTH_PASSWORD" Default="" Mode="" Description="Cypht Password" Type="Variable" Display="always" Required="false" Mask="true"></Config>
  <Config Name="Cypht DB Connection Type" Target="CYPHT_DB_CONNECTION_TYPE" Default="host" Mode="" Description="Cypht DB Connection Type" Type="Variable" Display="always" Required="false" Mask="false">host</Config>
  <Config Name="DB Host" Target="CYPHT_DB_HOST" Default="db" Mode="" Description="DB Host" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="DB User" Target="CYPHT_DB_USER" Default="cypht" Mode="" Description="DB User" Type="Variable" Display="always" Required="false" Mask="false">cypht</Config>
  <Config Name="Db Password" Target="CYPHT_DB_PASS" Default="" Mode="" Description="DB Password" Type="Variable" Display="always" Required="false" Mask="true"></Config>
  <Config Name="Session Type" Target="CYPHT_SESSION_TYPE" Default="DB" Mode="" Description="Session Type" Type="Variable" Display="always" Required="false" Mask="false">DB</Config>
  <Config Name="Db Name" Target="CYPHT_DB_NAME" Default="cypht" Mode="" Description="DB Name" Type="Variable" Display="always" Required="false" Mask="false">cypht</Config>
</Container>
